{
    "image": "{{ .ceilometer_compute_image }}",
    "user": "ceilometer",
    "restart": "always",
    "command": "kolla_start",
    "net": "host",
    "environment": {
        "KOLLA_CONFIG_STRATEGY":"COPY_ALWAYS",
        "KOLLA_CONFIG_FILE":"/var/lib/openstack/config/ceilometer-agent-compute.json"
    },
    "volumes": [
        "/var/lib/openstack/config/ceilometer:/var/lib/openstack/config/:ro",
        "/var/log/containers/ceilometer:/var/log/ceilometer:Z"
    ]
}
